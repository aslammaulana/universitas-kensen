{
  "extends": ["next/core-web-vitals", "eslint-config-prettier"],
  "plugins": ["import"],
  "rules": {
    "import/no-unused-modules": [1, { "unusedExports": true }],
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
    "no-unused-imports": "error"
  }
}
